<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="130"
        Width="300" Height="130"
        CanResize="False"
        x:Class="FishLevelEditor2.OpenProjectDialog"
        Title="Open Project">
  <StackPanel Height="" VerticalAlignment="Center" Spacing="10">
    <TextBlock HorizontalAlignment="Center" Grid.ColumnSpan="2" Grid.Row="0">Welcome to Fish Level Editor v2!</TextBlock>
    <Button Click="NewProjectButton_Click" Width="100" Name="NewProjectButton" HorizontalAlignment="Center" Content="New Project"></Button>
    <Button Click="LoadProjectButton_Click" Width="100" Name="LoadProjectButton" HorizontalAlignment="Center" Content="Load Project"></Button>
  </StackPanel>
</Window>
